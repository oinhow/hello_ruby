<p id="notice"><%= notice %></p>

<h1>喵一堆</h1>

<table>
  <thead>
    <tr>
      <th>描述</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pins.each do |pin| %>
      <tr>
        <td><%= pin.description %></td>
        <td><%= link_to '展示', pin %></td>
        <td><%= link_to '编辑', edit_pin_path(pin) %></td>
        <td><%= link_to '删除', pin, method: :delete, data: { confirm: '确定删除？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '新添加', new_pin_path %>
